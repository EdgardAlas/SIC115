RewriteEngine On

RewriteCond %{HTTP_HOST} ^www\.edgard2023\.me$ [NC]
RewriteRule ^(.*)$ https://edgard2023.me/$1 [R=301,L]

RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule ^(.*)$ index.php [QSA,L]

ErrorDocument 403 /views/error/403.php

Options -Indexes

<Files .htaccess>
Order allow,deny
Deny from all
</Files>
